
# YCA CRM Environment Variables
API_KEY=your_google_gemini_api_key_here
JWT_SECRET=your_jwt_secret_here
NODE_ENV=development
PORT=5000

# PostgreSQL Database Configuration (Neon)
SESSION_SECRET="your_session_secret_here"
DATABASE_URL="your_postgresql_connection_string_here"
PGDATABASE="your_database_name"
PGHOST="your_database_host"
PGPORT="5432"
PGUSER="your_database_user"
PGPASSWORD="your_database_password"

# Supabase Configuration (fallback)
SUPABASE_KEY=your_supabase_anon_key_here

# Neo Database Email Service Configuration
NEO_API_KEY=your_neo_database_api_key_here
EMAIL_FROM=noreply@ycacrm.com

# Instructions:
# 1. Copy this file to .env
# 2. Get your Google Gemini API key from https://makersuite.google.com/app/apikey
# 3. Replace 'your_google_gemini_api_key_here' with your actual API key
# 4. Get your PostgreSQL credentials from Replit Database tab
# 5. Get your Neo Database API key for email services
# 6. Configure remaining settings as needed
